{
  "name": "webtreemap-cli",
  "version": "2.1.2",
  "description": "treemap visualization",
  "main": "build/src/index.js",
  "bin": {
    "webtreemap": "build/src/cli.js",
    "treemap": "build/src/cli.js"
  },
  "dependencies": {
    "commander": "^8.1.0",
    "moo": "^0.5.1",
    "open": "^7.2.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/moo": "^0.5.5",
    "@types/node": "^14.6.4",
    "@types/tmp": "^0.2.0",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fmt": "prettier --write src/*.ts demo/*.ts *.md",
    "build": "tsc && webpack --mode=production",
    "demo": "du -a node_modules/ | awk '{print $1*512 \"\t\" $2}' | node build/src/cli.js --title 'node_modules for webtreemap'",
    "demo:tsc": "tsc --listFiles | node build/src/cli.js --title 'tsc --listFiles'",
    "demo:json": "node build/src/cli.js json-space package.json"
  },
  "files": [
    "build/**/*.js",
    "build/**/*.d.ts",
    "dist/webtreemap.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danvk/webtreemap.git"
  },
  "keywords": [
    "treemap",
    "visualization"
  ],
  "authors": [
    "Evan Martin <evan.martin@gmail.com>",
    "Dan Vanderkam <danvdk@gmail.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/danvk/webtreemap/issues"
  },
  "homepage": "https://github.com/danvk/webtreemap",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "avoid"
  }
}
